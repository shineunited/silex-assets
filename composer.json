{
	"name": "shineunited/silex-assets",
	"type": "library",
	"keywords": [
		"assets",
		"silex",
		"twig"
	],
	"license": "MIT",
	"require": {
		"silex/silex": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7",
		"symfony/browser-kit": "^3.2",
		"twig/twig": "^1.33"
	},
	"suggest": {
		"twig/twig": "Allow use of asset manager inside of twig templates"
	},
	"autoload": {
		"psr-4": {
			"ShineUnited\\Silex\\Assets\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ShineUnited\\Silex\\Assets\\Tests\\": "tests/"
		}
	}
}
